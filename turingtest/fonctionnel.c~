#include <stdio.h>
#include <stdlib.h>
unsigned char char2u(char c)
{
	return c-'0';
}


unsigned int str2u(const char* s, unsigned char n)
{
	///A complÃ©ter
	int i;
	int p=1;
	int res=0;
	for( i=0; i<n; i++) {
	p=p*10;
	res=char2u(s[i])*p;
	}
	return res;
}

int main()
{
	char s[]={'4','6','0','1',};
	int res=str2u(s,3);
	printf("%d",res);
	return 0;
}